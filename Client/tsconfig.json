{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "paths": {
      "source-map-js": [ "node_modules/source-map-js" ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5",
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules",
    "Gulpfile.js",
    "Gulp/src/js/index.js",
    "bin/**/*",
    "obj/**/*",
    "wwwroot/**/*",
    "**/*.js"
  ]
}
